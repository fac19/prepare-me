{
  "name": "prepare-me",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@nidi/html2canvas": "^1.0.0-rc.11",
    "@react-pdf/renderer": "^2.0.0-beta.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "canvas-to-buffer": "^1.1.0",
    "cloudinary-react": "^1.5.0",
    "jspdf": "^1.5.3",
    "react": "^16.13.1",
    "react-div-100vh": "^0.3.8",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-swipeable": "^5.5.1",
    "react-test-renderer": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "test": "CI=true react-scripts test --env=jsdom",
    "test:report": "react-scripts test --updateSnapshot --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "format": "prettier --write './src/*.js'",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec 'cypress/integration/*.spec.js'",
    "cy:record": "cypress run --record --key 527e4fa6-e949-4262-bba0-8d5a0578b865",
    "cy:ci": "start-server-and-test start http://localhost:3000 cy:run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.1",
    "@cypress/instrument-cra": "^1.1.1",
    "babel-plugin-istanbul": "^6.0.0",
    "canvas": "^2.6.1",
    "codecov": "^3.7.0",
    "cypress": "^4.8.0",
    "eslint-plugin-cypress": "^2.11.1",
    "husky": "^4.2.5",
    "istanbul-lib-coverage": "^3.0.0",
    "lint-staged": "^10.2.9",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "start-server-and-test": "^1.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && eslint src && npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint"
    ]
  },
  "nyc": {
    "exclude": "src/serviceWorker.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!src/serviceWorker.js"
    ],
    "coverageReporters": [
      "json"
    ]
  }
}
